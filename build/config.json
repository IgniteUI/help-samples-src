{
    "gitUrl": "https://damyanpetev.github.io/samples-test",
    "strings": {
        "resultEN": "Result",
        "resultJA": "Result"
    },
    "templateScriptTypes": [
        "text/html",
        "text/template",
        "text/x-ig-tmpl",
        "text/x-jquery-tmpl",
        "text/x-jsrender"
    ],
    "patterns": [
        {
            "match": "%%ignite-ui%%",
            "replace": "https://secure-cdn-na.infragistics.com/igniteui/latest"
        },
        {
            "match": "%%jquery%%",
            "replace": "https://code.jquery.com/jquery-1.9.1.min.js"
        },
        {
            "match": "%%jquery-ui%%",
            "replace": "https://code.jquery.com/ui/1.10.3/jquery-ui.min.js"
        },
        {
            "match": "%%modernizr%%",
            "replace": "https://ajax.aspnetcdn.com/ajax/modernizr/modernizr-2.8.3.js"
        },
        {
            "match": "%%angular%%",
            "replace": "https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js"
        },
        {
            "match": "%%bootstrap%%",
            "replace": "https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"
        },
        {
            "match": "%%bootstrap-css%%",
            "replace": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
        },
        {
            "match": "%%jsrender%%",
            "replace": "https://cdnjs.cloudflare.com/ajax/libs/jsrender/0.9.83/jsrender.min.js"
        },
        {
            "match": "%%knockout%%",
            "replace": "https://ajax.aspnetcdn.com/ajax/knockout/knockout-3.3.0.js"
        },
        {
            "match": "%%knockout.mapping%%",
            "replace": "https://cdnjs.cloudflare.com/ajax/libs/knockout.mapping/2.4.1/knockout.mapping.min.js"
        }
    ]
}